{
	"files.exclude": {
		"**/.git": true,
		"ent/{[!sp],?[!cr],??[!ho]}*": true,
		"ent/proto/khepri/horus/*.pb.go": true,
		"ent/proto/khepri/horus/horus*.go": true,
	},
	"files.readonlyInclude": {
		"ent/proto/khepri/horus/horus.proto": true,
	},
	"[proto3]": {
		"editor.formatOnSave": true
	},
	"protoc": {
		"options": [
			"--proto_path=${workspaceRoot}/ent/proto",
		]
	}
}
